   "source": [
    "def extract_age(age):\n",
    "    age_num = re.findall('[0-9]+',str(age))\n",
    "    if len(age_num) > 0:\n",
    "        return age_num[0]\n",
    "    else:\n",
    "        return age\n",
    "\n",
    "df['age']=df['age'].apply(lambda x: extract_age(x))"
   ]
